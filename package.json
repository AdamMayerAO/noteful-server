{
  "name": "noteful-server",
  "version": "1.0.0",
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require test/setup.js",
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "predeploy": "npm audit",
    "deploy": "git push heroku master",
    "postdeploy": "npm run migrate:production",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:production": "env SSL=true SSLMODE='require' DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate"
  },
  "keywords": [],
  "author": "Adam Mayer <r.adammayer@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "knex": "^0.21.2",
    "lodash": "^4.17.19",
    "morgan": "^1.10.0",
    "pg": "^7.4.3",
    "postgrator-cli": "^3.2.0",
    "xss": "^1.0.8"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  }
}
